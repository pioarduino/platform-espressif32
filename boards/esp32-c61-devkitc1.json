    "flash_size": "4MB",
    "maximum_ram_size": 327680,
    "maximum_size": 4194304,
    "require_upload_port": true,
    "speed": 460800
  },
  "espidf": {
    "custom_sdkconfig": [
      "CONFIG_IDF_TARGET=\"esp32c61\"" 
    ]
  },
  "url": "https://docs.espressif.com/projects/esp-dev-kits/en/latest/esp32c61/esp32-c61-devkitc-1/user_guide.html",
  "vendor": "Espressif"
